  },
  "dependencies": {
    "bluebird": "3.7.2",
    "colors": "1.4.0",
    "command-line-usage": "6.1.3",
    "jsonfile": "6.1.0",
    "semver": "7.5.4",
    "yargs": "17.7.1"
  },
  "devDependencies": {
